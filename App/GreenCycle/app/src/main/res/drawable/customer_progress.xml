<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background color for the remaining part -->
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/white" /> <!-- Change this to your desired background color -->
            <corners android:radius="4dp" /> <!-- Adjust the radius for rounded corners -->
            <stroke
                android:width="2dp"
                android:color="@color/mid_green" /> <!-- Change this to your desired outline color -->
        </shape>
    </item>

    <!-- Progress color -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/mid_green" /> <!-- Change this to your desired progress color -->
                <corners android:radius="4dp" /> <!-- Adjust the radius for rounded corners -->
                <stroke
                    android:width="2dp"
                    android:color="@color/mid_green" /> <!-- Change this to your desired outline color -->
            </shape>
        </clip>
    </item>
</layer-list>